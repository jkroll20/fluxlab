<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="fluxdemo" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="./fluxdemo" prefix_auto="1" extension_auto="1" />
				<Option object_output="./obj/Debug/" />
				<Option external_deps="libflux/lib/libflux-gl_sdl.a;" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="./fluxdemo" prefix_auto="1" extension_auto="1" />
				<Option object_output="./obj/Release/" />
				<Option external_deps="libflux/lib/libflux-gl_sdl.a;" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option use_console_runner="0" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
			<Add directory="libflux/src" />
		</Compiler>
		<Linker>
			<Add library="flux-gl_sdl" />
			<Add library="GL" />
			<Add library="SDL" />
			<Add library="freeimage" />
			<Add library="GLU" />
			<Add library="Cg" />
			<Add library="CgGL" />
			<Add library="glut" />
			<Add library="SDL_image" />
			<Add directory="libflux/lib" />
		</Linker>
		<ExtraCommands>
			<Add before="make -Clibflux" />
			<Add before="(rm fluxdemo || true) 2&gt;/dev/null" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="cg/colors.cg" />
		<Unit filename="cg/displace.cg" />
		<Unit filename="cg/plasma.cg" />
		<Unit filename="cg/test.cg" />
		<Unit filename="cg/test2.cg" />
		<Unit filename="src/fbo.h" />
		<Unit filename="src/main.cpp" />
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
